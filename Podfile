  pod 'Masonry'
  pod 'SSZipArchive'
  pod 'YYWebImage', :git => 'https://hub.fgit.cf/ibireme/YYWebImage'
  pod 'MBProgressHUD', :git => 'https://hub.fgit.cf/jdg/MBProgressHUD'

end
